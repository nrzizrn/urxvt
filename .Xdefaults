    ! black
    urxvt*color:    #141414
    urxvt*color8:   #5C585C
    ! red
    urxvt*color1:   #F54E00
    urxvt*color9:   #FF0000
    ! green
    urxvt*color2:   #45ED6F
    urxvt*color10:  #00FF00
    ! yellow
    urxvt*color3:   #C4DE50
    urxvt*color11:  #DDFF00
    ! blue
    urxvt*color4:   #4756F5
    urxvt*color12:  #002BFF
    ! magenta
    urxvt*color5:   #EE69FA
    urxvt*color13:  #FF00DD
    ! cyan
    urxvt*color6:   #6CF7EB
    urxvt*color14:  #07B099
    ! white
    urxvt*color7:   #D6D6D6
    urxvt*color15:  #FFFFFF
	urxvt.background: #1c1c1c
    urxvt.background: #000000
	urxvt.foreground: 15
    urxvt.foreground: #00EE00

    !transparency
    ! real trans
    !urxvt*depth: 32
    !urxvt*background: rgba:0000/0000/0200/c800
    ! fake trans
    urxvt*transparent: true
    urxvt*shading: 30
